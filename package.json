{
  "name": "page-gen",
  "type": "module",
  "private": true,
  "scripts": {
    "dev": "bun run --watch src/server/index.ts",
    "dev:client": "vite --open",
    "build": "vite build && bun build src/server/index.ts --outdir dist/server --target bun",
    "preview": "vite preview",
    "test": "bun test",
    "db:generate": "drizzle-kit generate",
    "db:migrate": "drizzle-kit migrate",
    "typecheck": "tsc --noEmit",
    "dev:docker": "bunx open-cli http://localhost:5173 && docker compose up --build",
    "dev:docker:d": "docker compose up --build -d && bunx open-cli http://localhost:5173",
    "logs:viewer": "bun --hot scripts/logs-viewer.ts"
  },
  "devDependencies": {
    "@tailwindcss/vite": "^4.2.0",
    "@types/bun": "^1.3.9",
    "@types/react": "^19.2.14",
    "@types/react-dom": "^19.2.3",
    "@vitejs/plugin-react": "^5.1.4",
    "drizzle-kit": "^0.31.9",
    "tailwindcss": "^4.2.0",
    "typescript": "^5.9.3",
    "vite": "^7.3.1"
  },
  "dependencies": {
    "@ai-sdk/anthropic": "^3.0.45",
    "@ai-sdk/deepseek": "^2.0.20",
    "@ai-sdk/google": "^3.0.29",
    "@ai-sdk/mistral": "^3.0.20",
    "@ai-sdk/openai": "^3.0.29",
    "@ai-sdk/xai": "^3.0.57",
    "@codemirror/lang-css": "^6.3.1",
    "@codemirror/lang-html": "^6.4.11",
    "@codemirror/lang-javascript": "^6.2.4",
    "@codemirror/lang-json": "^6.0.2",
    "@codemirror/lang-markdown": "^6.5.0",
    "@hono/node-server": "^1.19.9",
    "@radix-ui/react-collapsible": "^1.1.12",
    "@radix-ui/react-dialog": "^1.1.15",
    "@radix-ui/react-scroll-area": "^1.2.10",
    "@radix-ui/react-select": "^2.2.6",
    "@radix-ui/react-separator": "^1.1.8",
    "@radix-ui/react-slot": "^1.2.4",
    "@radix-ui/react-tabs": "^1.1.13",
    "@radix-ui/react-toggle": "^1.1.10",
    "@radix-ui/react-tooltip": "^1.2.8",
    "@uiw/react-codemirror": "^4.25.4",
    "ai": "^6.0.90",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "drizzle-orm": "^0.45.1",
    "fflate": "^0.8.2",
    "hono": "^4.11.10",
    "lucide-react": "^0.575.0",
    "nanoid": "^5.1.6",
    "radix-ui": "^1.4.3",
    "react": "^19.2.4",
    "react-dom": "^19.2.4",
    "react-markdown": "^10.1.0",
    "remark-gfm": "^4.0.1",
    "tailwind-merge": "^3.5.0",
    "tw-animate-css": "^1.4.0",
    "zod": "^4.3.6",
    "zustand": "^5.0.11"
  }
}
